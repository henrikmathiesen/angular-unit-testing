{
  "name": "angular-unit-testing",
  "version": "1.0.0",
  "description": "Do it til you love it. Following along: https://www.pluralsight.com/courses/angularjs-ngmock-unit-testing",
  "main": "index.js",
  "scripts": {
    "postinstall": "npm run copy-fonts",
    "test": "npm run template-cache && npm run uglifyjs-app && karma start karma.conf.js",
    "start": "npm run uglifyjs-lib && npm run eslint && npm run template-cache && npm run uglifyjs-app && npm run less-app-lib && concurrently \"npm run eslint-watch\" \"npm run template-cache-watch\" \"npm run uglifyjs-app-watch\" \"npm run watch-less-app\" \"npm run serve\"",
    "prod": "npm run delete-build-files && npm run uglifyjs-lib-prod && npm run template-cache && npm run uglifyjs-app-prod && npm run less-prod",
    "serve": "http-server -p 1337",
    "delete-build-files": "del-cli bld/*.css bld/*.js bld/*.map",
    "copy-fonts": "copyfiles -u 1 \"node_modules/font-awesome/fonts/**/*\" bld",
    "uglifyjs-lib": "uglifyjs node_modules/jquery/dist/jquery.js node_modules/angular/angular.js node_modules/angular-route/angular-route.js node_modules/angular-resource/angular-resource.js node_modules/angular-ui-bootstrap/dist/ui-bootstrap-tpls.js -o bld/lib.js --source-map bld/lib.js.map --source-map-url /bld/lib.js.map --source-map-include-sources",
    "uglifyjs-app": "uglifyjs app/js/*.js app/js/services/*.js app/js/controllers/*.js -o bld/app.js --source-map bld/app.js.map --source-map-url /bld/app.js.map --source-map-include-sources",
    "uglifyjs-app-watch": "onchange \"app/js/**/*.js\" -- npm run uglifyjs-app",
    "eslint": "eslint app/js/**/*.js",
    "eslint-watch": "onchange \"app/js/**/*.js\" -- npm run eslint",
    "less-app-lib": "lessc --source-map-map-inline --source-map-less-inline app/less/app.less bld/app.css && postcss -u autoprefixer --autoprefixer.browsers \"last 3 versions\" -r bld/app.css",
    "watch-less-app": "onchange \"app/less/**/*.less\" -- npm run less-app-lib",
    "template-cache": "nghtml2js -f \"app/templates/*.html\" -o bld/template.js",
    "template-cache-watch": "onchange \"app/templates/*.html\" -- npm run template-cache",
    "": "================================================================================================================================================================",
    "uglifyjs-lib-prod": "uglifyjs node_modules/jquery/dist/jquery.js node_modules/angular/angular.js node_modules/angular-route/angular-route.js node_modules/angular-resource/angular-resource.js node_modules/angular-ui-bootstrap/dist/ui-bootstrap-tpls.js -o bld/lib.js -c -m --comments",
    "uglifyjs-app-prod": "uglifyjs app/js/*.js app/js/services/*.js app/js/controllers/*.js -o bld/app.js",
    "less-prod": "lessc --clean-css app/less/app.less bld/app.css && postcss -u autoprefixer --autoprefixer.browsers \"last 3 versions\" -r bld/app.css"
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "env": {
      "browser": true,
      "jquery": true
    },
    "parserOptions": {
      "ecmaVersion": 5,
      "sourceType": "script"
    },
    "globals": {
      "jQuery": true,
      "$": true,
      "angular": true
    },
    "rules": {
      "no-console": 0,
      "no-debugger": 0,
      "no-unused-vars": 0
    }
  },
  "eslint-rules": "http://eslint.org/docs/rules/",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/henrikmathiesen/angularUnitTesting.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/henrikmathiesen/angularUnitTesting/issues"
  },
  "homepage": "https://github.com/henrikmathiesen/angularUnitTesting#readme",
  "devDependencies": {
    "angular-template-cache": "^1.1.0",
    "autoprefixer": "^6.4.1",
    "concurrently": "^2.2.0",
    "copyfiles": "^1.0.0",
    "del-cli": "^0.2.0",
    "eslint": "^3.6.0",
    "font-awesome": "^4.6.3",
    "http-server": "^0.9.0",
    "jasmine": "^2.5.2",
    "karma": "^1.3.0",
    "karma-cli": "^1.0.1",
    "karma-jasmine": "^1.0.2",
    "karma-phantomjs-launcher": "^1.0.2",
    "less": "^2.7.1",
    "less-plugin-clean-css": "^1.5.1",
    "onchange": "^3.0.2",
    "postcss-cli": "^2.6.0",
    "typings": "^1.4.0",
    "uglify-js": "^2.7.3"
  },
  "dependencies": {
    "angular": "^1.5.8",
    "angular-mocks": "^1.5.8",
    "angular-resource": "^1.5.8",
    "angular-route": "^1.5.8",
    "angular-ui-bootstrap": "^2.1.4",
    "bootstrap": "^3.3.7",
    "jquery": "^3.1.1",
    "normalize.less": "^1.0.0"
  }
}
