{
  "name": "angular-unit-testing",
  "version": "1.0.0",
  "description": "Do it til you love it. Following along: https://www.pluralsight.com/courses/angularjs-ngmock-unit-testing",
  "main": "index.js",
  "scripts": {
    "start": "npm run uglifyjs && concurrently \"npm run uglifyjs-watch\" \"npm run serve\"",
    "uglifyjs-watch": "onchange \"app/js/**/*.js\" -- npm run uglifyjs",
    "uglifyjs": "uglifyjs app/js/*.js -o bld/app.js --source-map bld/app.js.map --source-map-url /bld/app.js.map --source-map-include-sources",
    "serve": "http-server -p 1337",
    "delete-map-files": "del-cli bld/*.map"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/henrikmathiesen/angularUnitTesting.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/henrikmathiesen/angularUnitTesting/issues"
  },
  "homepage": "https://github.com/henrikmathiesen/angularUnitTesting#readme",
  "devDependencies": {
    "concurrently": "^2.2.0",
    "del-cli": "^0.2.0",
    "http-server": "^0.9.0",
    "onchange": "^3.0.2",
    "uglify-js": "^2.7.3"
  }
}
